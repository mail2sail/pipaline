pipeline{
    agent any
    stages
    {
        stage('Build')
        {
            steps{
                echo "Building the Project.........."
            }
        }
        stage('Code Scan')
        {
            steps{
                echo "Scanning code of the Project.........."
            }
        }
        stage('Unit Test')
        {
            steps{
                echo "Unit testing the Project.........."
            }
        }
        stage('QA Deploy')
        {
            steps{
                echo "Deploying the Project in QA Environment.........."
            }
        }
        stage('QA Test') 
        {
            steps{
                echo "QA Testing the Project.........."
            }
        }
        stage('Production Deploy')
        {
            steps{
                echo "Deploying the Project in Production.........."
            }
        }
    }
}
